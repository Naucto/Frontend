{
    "compilerOptions": {
        "allowJs": true,
        "esModuleInterop": true,
        "baseUrl": "./",
        "paths": {
            "@assets/*": [
                "./src/assets/*"
            ],
            "@modules/*": [
                "./src/modules/*"
            ],
            "@components/*": [
                "./src/components/*"
            ],
            "@shared/*": [
                "./src/shared/*"
            ],
            "@utils/*": [
                "./src/utils/*"
            ],
            "@theme/*": [
                "./src/theme/*"
            ],
            "@lib/*": [
                "./lib/*"
            ],
            "@api": [
                "./src/api"
            ],
            "@api/*": [
                "./src/api/*"
            ],
            "@providers/*": [
                "./src/providers/*"
            ]
        }
    }
}